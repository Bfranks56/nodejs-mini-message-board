<html>
  <head>
    <title>Mini Message Board</title>
  </head>
  <body>
    <div>
        <% messages.forEach((message)=> { %>
            <div style="border: 1px solid black; padding: 10px; margin-bottom: 10px;">
                <h3><%= message.user %></h3>
                <p><%= message.text %></p>
                <i>Posted on: <%= message.added %></i>
            </div>
            <% }) %>
        </div>
  </body>
</html>
